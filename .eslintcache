        id={`${obj.id}`}\n              />\n            );\n          })}\n        </Row>\n        <small>Advertisement</small>\n        <div id=\"container-0598f6fa244fae4bfda38c266a87dda5\"></div>\n      </Container>\n    </div>\n  );\n}\n\nexport default Home;","/Users/heinmynn/web/sports/src/components/Standings.js",[],"/Users/heinmynn/web/sports/src/components/Fixtures.js",[],"/Users/heinmynn/web/sports/src/components/Lives.js",[],"/Users/heinmynn/web/sports/src/components/Results.js",[],"/Users/heinmynn/web/sports/src/components/Request.js",[],"/Users/heinmynn/web/sports/src/components/Tracking.js",[],"/Users/heinmynn/web/sports/src/components/Filter.js",["91"],"/Users/heinmynn/web/sports/src/components/Player.js",["92"],"import React, { useEffect, useState } from \"react\";\nimport { Tabs, Tab, Button, Alert } from \"react-bootstrap\";\nimport ReactHlsPlayer from \"react-hls-player\";\nimport { useHistory, useParams } from \"react-router-dom\";\nimport Iframe from \"react-iframe\";\nimport app from '../firebase.config';\n\nfunction Player(props) {\n  const [data, setData] = useState([]);\n  const [loading, setLoading] = useState(true);\n  const [pause, setPause] = useState(true);\n  const { id } = useParams();\n  let history = useHistory();\n\n  const db=app.firestore();\n  // const match = data.filter((match) => match.key === id);\n  let i = 0;\n\n  const MatchesCheck = () => {\n    if (!data.stream && loading === false) {\n      return (\n        <Alert variant=\"warning\">\n          <Alert.Heading>Finding Live Streaming ... </Alert.Heading>\n          Live Stream will be available <b>1 HOUR</b> before the match.{\" \"}\n        </Alert>\n      );\n    } else {\n      return null;\n    }\n  };\n\n  const fetchMatch=async()=>{\n    const response=db.collection('matches').doc(id);\n    response.get().then((querySnapshot) => {\n        setData(querySnapshot.data())\n        setLoading(false);\n    })\n}\nuseEffect(() => {\n    fetchMatch();\n}, []);\n  return